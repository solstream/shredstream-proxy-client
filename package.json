{
  "name": "@solstream/shredstream-proxy",
  "version": "0.0.2",
  "description": "TypeScript gRPC client for Jito Shredstream Proxy.",
  "type": "module",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "prepare": "husky",
    "lint": "eslint .",
    "format": "prettier --write .",
    "gen:proto": "protoc --ts_out src/generated --ts_opt=eslint_disable,generate_dependencies --proto_path=proto proto/shredstream.proto",
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "typecheck": "tsc --noEmit",
    "dev": "tsup --watch",
    "build": "tsup",
    "release": "pnpm build && pnpm changeset publish"
  },
  "keywords": [
    "solana",
    "jito",
    "shredstream",
    "grpc",
    "protobuf",
    "typescript"
  ],
  "license": "MIT",
  "packageManager": "pnpm@9.15.0",
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint . --fix",
      "prettier . --write"
    ],
    "**/*.{json,md,yaml,yml}": "prettier . --write"
  },
  "devDependencies": {
    "@changesets/cli": "2.27.10",
    "@protobuf-ts/plugin": "^2.11.1",
    "@types/eslint": "9.6.1",
    "@types/eslint__js": "8.42.3",
    "@types/node": "22.10.2",
    "eslint": "9.16.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "husky": "9.1.7",
    "lint-staged": "15.2.11",
    "prettier": "3.4.2",
    "tsup": "8.3.5",
    "tsx": "4.19.2",
    "typescript": "5.7.2",
    "typescript-eslint": "8.18.0",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "2.1.8"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.13.4",
    "@protobuf-ts/grpc-transport": "^2.11.1",
    "@protobuf-ts/runtime": "^2.11.1",
    "@protobuf-ts/runtime-rpc": "^2.11.1"
  }
}
